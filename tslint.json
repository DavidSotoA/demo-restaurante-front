{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "ordered-imports": [true, {
      "import-sources-order": "any",
      "named-imports-order": "any"
    }],
    "interface-name" : false
  },
  "jsRules": {
    "no-console": false 
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
